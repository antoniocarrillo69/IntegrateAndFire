  -I"C:\Archivos de programa\Borland\CBuilder4\Projects\";..;$(BCB)\include;$(BCB)\include\vcl \
  -$Y- -$L- -$D- -v -JPHNE -M
RFLAGS = -i"C:\Archivos de programa\Borland\CBuilder4\Projects\";..;$(BCB)\include;$(BCB)\include\vcl
